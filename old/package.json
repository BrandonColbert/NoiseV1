{
	"name": "noise",
	"version": "0.1.0",
	"description": "Configurable playlist creator",
	"main": "js/main.js",
	"scripts": {
		"start": "electron .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"pack": "electron-builder --dir",
		"dist": "electron-builder"
	},
	"build": {
		"productName": "Noise",
		"appId": "exn.database.noise",
		"win": {
			"target": [
				"portable"
			],
			"icon": "resources/icons/icon.png"
		},
		"portable": {
			"artifactName": "${productName} ${version}.${ext}"
		},
		"directories": {
			"output": "dist",
			"buildResources": "dist/buildResources"
		}
	},
	"author": "Brandon Colbert",
	"url": "https://brandoncolbert.github.io/projects/noise.html",
	"license": "ISC",
	"devDependencies": {
		"electron": "^8.2.5",
		"electron-builder": "^22.7.0"
	},
	"dependencies": {
		"@cliqz/adblocker-electron": "^1.15.1",
		"cross-fetch": "^3.0.4",
		"uuid": "^8.1.0"
	}
}
